<!DOCTYPE html>
<h1 style="text-align:center">Accidents</h1>

<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">name</th>
                <th scope="col">Longitude</th>
                <th scope="col">Latitude</th>
                <th scope="col">time</th>
            </tr>

        </thead>
        <tbody>
            {{#each crash}}

                <tr>
                    <td>{{this.name}}</td>
                    <td>{{this.long}}</td>
                    <td>{{this.lat}}</td>
                    <td>{{this.time}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>
